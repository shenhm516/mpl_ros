
<launch>
  <node pkg="planning_ros_utils"
    type="cloud_to_map"
    name=""
    output="screen">
    <remap from="~cloud" to="cloud"/>
    <remap from="~voxel_map" to="voxel_map"/>
    <param name="resolution" value="0.4"/>
    <param name="origin_x" value="-7.0"/>
    <param name="origin_y" value="-18.0"/>
    <param name="origin_z" value="0.0"/>
    <param name="range_x" value="55.0"/>
    <param name="range_y" value="40.0"/>
    <param name="range_z" value="5.0"/>
  </node>
</launch>
